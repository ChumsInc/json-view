{
  "name": "json-view",
  "version": "1.0.4",
  "description": "Chums Common Components",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --progress",
    "test": "webpack serve --config webpack.test.js --progress",
    "copy-css": "copyfiles --verbose --up 1 src/**/*.css dist/ && copyfiles --verbose --up 1 src/**/*.scss dist/",
    "clean": "rimraf dist/",
    "compile": "tsc",
    "build": "npm run clean && npm run compile && npm run copy-css",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UtahGooner/json-view.git"
  },
  "author": "Steve Montgomery",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UtahGooner/json-view/issues"
  },
  "homepage": "https://github.com/UtahGooner/json-view#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.1",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^4.1.2",
    "sass": "^1.32.10",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.0.0",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.0.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "css-loader": "^6.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "style-loader": "^3.2.1"
  },
  "auto-changelog": {
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false,
    "breakingPattern": "BREAKING CHANGE:"
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/base16": "^1.0.2",
    "base16": "^1.0.0",
    "classnames": "^2.3.1"
  }
}
